    <script>
        $('document').ready(function () {
            $('.date-server').each(function () {
                let epocValue = $(this).text();
                let d = new Date(0);
                d.setUTCSeconds(epocValue);

                $(this).text(d.toLocaleString())

            });
        });

        $(function () {
            var imagesPreview = function (input, placeToInsertImagePreview) {

                if (input.files) {
                    var filesAmount = input.files.length;

                    for (i = 0; i < filesAmount; i++) {
                        var reader = new FileReader();

                        reader.onload = function (event) {
                            $($.parseHTML('<img>')).attr('src', event.target.result).appendTo(placeToInsertImagePreview);
                        }

                        reader.readAsDataURL(input.files[i]);
                    }
                }

            };

            $('.media-img').on('change', function () {
                imagesPreview(this, 'div.preview');
            });

            $('.clear-media').on('click', function () {
                $('.media-img').val('');
                $('.preview').html('');
            });
        });

        $('#deleteRoomModal').on('show.bs.modal', function (event) {
            var button = $(event.relatedTarget)
            var recipient = button.data('id')

            var modal = $(this)
            modal.find('#room-id').attr("value", recipient)
        })

        $('#deleteFormModal').on('show.bs.modal', function (event) {
            var button = $(event.relatedTarget)
            var recipient = button.data('id')

            var modal = $(this)
            modal.find('#delete-id').attr("value", recipient)
        })

        $('#itemStatusModal').on('show.bs.modal', function (event) {
            var button = $(event.relatedTarget)
            var recipient = button.data('id')
            if (recipient == null) {
                var modal = $(this)
                modal.find('#status-id').attr("value", null)
                modal.find('#Code').val('')
                modal.find('#Status').val('')
                modal.find('#Decription').val('')
            } else {
                var modal = $(this)
                modal.find('#status-id').attr("value", recipient)
                modal.find('#Code').val(button.data('code'))
                modal.find('#Status').val(button.data('status'))
                modal.find('#Decription').val(button.data('desc'))
            }
        })

        $('#itemCategoriesModal').on('show.bs.modal', function (event) {
            var button = $(event.relatedTarget)
            var recipient = button.data('id')
            if (recipient == null) {
                var modal = $(this)
                modal.find('#category-id').attr("value", null)
                modal.find('#Name').val('')
                modal.find('#Description').val('')
            } else {
                var modal = $(this)
                modal.find('#category-id').attr("value", recipient)
                modal.find('#Name').val(button.data('name'))
                modal.find('#Description').val(button.data('desc'))
            }
        })

        
    </script>
